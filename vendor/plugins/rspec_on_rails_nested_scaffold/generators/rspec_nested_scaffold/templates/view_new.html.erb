<h1>New <%= singular_name %></h1>

<%%= error_messages_for :<%= singular_name %> %>

<%% form_for(:<%= singular_name %>, @<%= singular_name %>, :url => <%= nesting_owner %>_<%= plural_name %>_path(@<%= nesting_owner %>)) do |f| %>
<% for attribute in attributes -%>
  <p>
		<%%= f.label :<%= attribute.name %> %><br />
    <%%= f.<%= attribute.field_type %> :<%= attribute.name %> %>
  </p>
<% end -%>
  <p>
    <%%= f.submit "Create" %>
  </p>
<%% end %>

<%%= link_to 'Back', <%= nesting_owner %>_<%= plural_name %>_path(@<%= nesting_owner %>) %>